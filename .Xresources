!!look and feel
xterm.termName: xterm-256color
xterm.geometry: 80x36
xterm*scrollBar: false
xterm*rightScrollBar: true
xterm*loginshell: true
xterm*cursorBlink: true
!!xterm*background:  black
!!xterm*foreground:   #00d75f
xterm.borderLess: true
xterm.cursorBlink: true
xterm*colorUL: yellow
xterm*colorBD: white
!!fix alt key input 
xterm*eightBitInput: false
xterm*altSendsEscape: true
!!font and locale
xterm.utf8:     true
xterm*utf8Title: true
xterm*fontMenu*fontdefault*Label: Default
xterm*faceName: Monaco:antialias=True:pixelsize=12
!xterm*faceName: Monospace:antialias=True:pixelsize=10
xter*boldFont: Monaco:style=Normal:pixelsize=12
!xter*boldFont: Monospace:style=Normal:pixelsize=10
xterm*faceNameDoublesize: WenQuanYi Micro Hei:pixelsize=12:antialias=True
xTerm*locale: zh_CN.UTF-8
*VT100*translations: #override \n\
XTerm*selectToClipboard: true
Shift <KeyPress> Insert:insert-selection(CLIPBOARD, CUT_BUFFER1) \n\
~Shift~Ctrl<Btn2Up>: insert-selection(PRIMARY, CUT_BUFFER0) \n\
~Shift<BtnUp>: select-end(CLIPBOARD, CUT_BUFFER1) \n\
~Shift<BtnUp>: select-end(PRIMARY, CUT_BUFFER0)
xTerm*inputMethod: ibus
!*VT100.utf8Fonts.font: -misc-fixed-medium-r-normal--18-120-100-100-c-90-iso10646-1
!xterm*faceName: DejaVu Sans Mono:antialias=True:pixelsize=17
xterm*xftAntialias: true
xterm*cjkWidth:false
xterm*selectToClipboard:true

xterm*background:              #002b36
xterm*foreground:              #839496
xterm*fading:                  40
xterm*fadeColor:               #002b36
xterm*cursorColor:             #93a1a1
